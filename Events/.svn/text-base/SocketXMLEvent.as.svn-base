package Events {
	import flash.events.Event;
	
	public class SocketXMLEvent extends Event{
		static public var SOCKET_XML:String = "SocketXML";
		public var xml:XML;
			
		public function SocketXMLEvent(x:XML, type:String, bubbles:Boolean = false, cancelable:Boolean = false){
			super(type, bubbles,cancelable);
			xml=x;
		}
	}
}